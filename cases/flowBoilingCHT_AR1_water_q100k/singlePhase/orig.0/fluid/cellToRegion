{
    wall_bottom
    {
        type            zeroGradient;
    }
    wall_symm
    {
        type            symmetry;
    }
    plexi
    {
        type            zeroGradient;
    }
    symm
    {
        type            symmetry;
    }
    outlet
    {
        type            zeroGradient;
    }
    inlet
    {
        type            zeroGradient;
    }
    solid_to_fluid
    {
        type            calculated;
        value           uniform 0;
    }
    adiabaticWall
    {
        type    zeroGradient;
    }    symm

}
