FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      mDot;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{    
    outlet
    {
        type            zeroGradient;
    }

    faceWall
    {
        type            zeroGradient;
    }

}

