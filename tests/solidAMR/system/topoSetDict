FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      topoSetDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


actions
(
    {
        name    fluidCellSet;
        type    cellSet;
        action  new;
        source  boxToCell;
        box     (0 0 0.0005) (0.003 0.003 0.003);
    }
    {
        name    fluid;
        type    cellZoneSet;
        action  new;
        source  setToCellZone;
        set     fluidCellSet;
    }

    {
        name    solidCellSet;
        type    cellSet;
        action  new;
        source  cellToCell;
        set     fluidCellSet;
    }
    {
        name solidCellSet;
        type cellSet;
        action invert;
    }
    {
        name    solid;
        type    cellZoneSet;
        action  new;
        source  setToCellZone;
        set     solidCellSet;
    }

);


