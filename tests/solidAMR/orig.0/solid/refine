FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      refine;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    solid_to_fluid
    {
        type            mappedFixedInternalValue;
        sampleRegion    fluid;
        sampleMode      nearestPatchFace;
        samplePatch     fluid_to_solid;
        offset          (0 0 0);
        field           alpha.liquid;
        setAverage      no;
        average         0;
        value           $internalField;

    }

    sides
    {
        type    zeroGradient;
    }

    bottom
    {
        type            zeroGradient;
    }

}

// ************************************************************************* //
