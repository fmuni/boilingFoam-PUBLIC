FoamFile
{
    version     2.0;
    format      ascii;
    class       surfaceScalarField;
    object      meshPhi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 3 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    sides
    {
        type    calculated;
        value   $internalField;
    }

    bottom
    {
        type   calculated;
        value   $internalField;
    }

    solid_to_fluid
    {
        type    calculated;
        value   $internalField;
    }
}

