FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField  uniform 1e5;

boundaryField
{
    sides
    {
        type    zeroGradient;
    }

    top
    {
        type            fixedValue;
        value           uniform 1e5;
    }

    fluid_to_solid
    {
        type    zeroGradient;
    }
}

