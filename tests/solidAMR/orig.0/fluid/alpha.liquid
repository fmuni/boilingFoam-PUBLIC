FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      alpha.water;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    sides
    {
        type            zeroGradient;
        gradient        uniform 0;
        limit           gradient;
        theta0          30;
        value           uniform 1;
    }

    fluid_to_solid
    {
        type            zeroGradient;
        gradient        uniform 0;
        limit           gradient;
        theta0          30;
        value           uniform 1;
    }

    top
    {
        type        zeroGradient;
    }
}

