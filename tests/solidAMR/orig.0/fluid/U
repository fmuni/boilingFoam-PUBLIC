FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField  uniform (0 0 0);

boundaryField
{
    sides
    {
        type    noSlip;
    }

    top
    {
        type    zeroGradient;
    }

    fluid_to_solid
    {
        type noSlip;
    }
}

